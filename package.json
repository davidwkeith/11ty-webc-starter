{
  "name": "@dwk/11ty-webc-starter",
  "version": "0.1.0",
  "description": "A style-free starter for templating an entire 11ty site using WebC",
  "type": "module",
  "scripts": {
    "serve": "npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy",
    "test": "npm run build",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidwkeith/11ty-webc-starter.git"
  },
  "keywords": [
    "11ty",
    "template",
    "webc"
  ],
  "author": {
    "name": "David W. Keith",
    "email": "git@dwk.io",
    "url": "https://dwk.io/"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidwkeith/11ty-webc-starter/issues"
  },
  "homepage": "https://github.com/davidwkeith/11ty-webc-starter#readme",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.webc": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-fetch": "^5.1.0",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "dotenv": "^17.2.0",
    "html-minifier-terser": "^7.2.0",
    "jsonld-lint": "^0.3.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2"
  }
}
